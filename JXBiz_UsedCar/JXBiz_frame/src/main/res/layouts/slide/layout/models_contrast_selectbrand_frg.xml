<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/models_contrast_drawer_layout_carseries"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/models_contrast_rl_add_car"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <include layout="@layout/titlebar_otherd_feault" />

        <FrameLayout
            android:id="@+id/models_contrast_ll_select_brand"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:gravity="center"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="vertical">

                <com.etong.android.frame.widget.pinnedlistview.PinnedSectionListView
                    android:id="@+id/models_contrast_lv_brand_list"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/white"
                    android:divider="@null"
                    android:scrollbars="none" />
            </LinearLayout>


            <TextView
                android:id="@+id/dialog"
                android:layout_width="80.0dip"
                android:layout_height="80.0dip"
                android:layout_gravity="center"
                android:background="@drawable/find_car_show_head_toast_bg"
                android:gravity="center"
                android:textColor="#ffffffff"
                android:textSize="30.0dip"
                android:visibility="invisible" />

            <com.etong.android.frame.widget.sortlistview.EtongSideBar
                android:id="@+id/models_contrast_sidebar"
                android:layout_width="30.0dip"
                android:layout_height="fill_parent"
                android:layout_gravity="right|center"
                android:background="#FFF" />


        </FrameLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="end"
        android:layout_marginLeft="-65dp"
        android:background="@color/white"
        android:clickable="true"
        android:orientation="vertical">

        <FrameLayout
            android:id="@+id/models_contrast_fly_drawer_carseries"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:foregroundGravity="right" />

        <FrameLayout
            android:id="@+id/models_contrast_fly_drawer_collect"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:foregroundGravity="right"
            android:visibility="gone" />
    </LinearLayout>


</android.support.v4.widget.DrawerLayout>
